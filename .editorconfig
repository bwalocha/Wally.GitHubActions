root = true

[*]
charset = utf-8
end_of_line = crlf
indent_style = space
indent_size = 4
insert_final_newline = true
tab_width = 4
trim_trailing_whitespace = false

[*.vue]
indent_size = 2

[{*.har,*.inputactions,*.js,*.jsb2,*.jsb3,*.json,.babelrc,.eslintrc,.stylelintrc,bowerrc,jest.config}]
indent_size = 2

[*.{yaml,yml}]
indent_size = 2
trim_trailing_whitespace = true

[*.{tf,tfvars}]
indent_size = 2
trim_trailing_whitespace = true

[*.{htm,html,ng,sht,shtm,shtml,xml,config,nuspec,resx,vsixmanifest,csproj,targets,props}]
space_before_self_closing = true
resharper_xml_space_before_self_closing = true
resharper_space_before_self_closing = true
html_space_inside_empty_tag = true
xml_space_inside_empty_tag = true
resharper_html_space_before_self_closing = true
html_space_before_self_closing = true
resharper_html_space_after_last_attribute = true
html_space_after_last_attribute = true
resharper_space_after_last_attribute = true
space_after_last_attribute = true
ij_html_space_inside_empty_tag = true
ij_html_text_wrap = normal
html_text_wrap = normal

[*.cs]
indent_style = tab

# Code style rules
# https://docs.microsoft.com/en-us/dotnet/fundamentals/code-analysis/style-rules/

# Index of EditorConfig properties
# https://www.jetbrains.com/help/resharper/EditorConfig_Index.html

# dotnet
dotnet_naming_rule.camel_case_for_private_internal_fields.severity = suggestion
dotnet_naming_rule.camel_case_for_private_internal_fields.style = camel_case_underscore_style
dotnet_naming_rule.camel_case_for_private_internal_fields.symbols = private_internal_fields
dotnet_naming_style.camel_case_underscore_style.capitalization = camel_case
dotnet_naming_style.camel_case_underscore_style.required_prefix = _
dotnet_naming_symbols.private_internal_fields.applicable_accessibilities = private, internal
dotnet_naming_symbols.private_internal_fields.applicable_kinds = field
dotnet_separate_import_directive_groups = true
dotnet_sort_system_directives_first = true
dotnet_style_namespace_match_folder = true

# csharp
csharp_align_first_arg_by_paren = false
csharp_align_multiline_array_and_object_initializer = false
csharp_braces_for_dowhile = warning
csharp_braces_for_fixed = warning
csharp_braces_for_for = warning
csharp_braces_for_foreach = warning
csharp_braces_for_ifelse = warning
csharp_braces_for_lock = warning
csharp_braces_for_using = warning
csharp_braces_for_while = warning
csharp_continuous_indent_multiplier = 1
csharp_wrap_before_first_type_parameter_constraint = true
csharp_wrap_before_type_parameter_langle = true
csharp_wrap_parameters_style = chop_if_long

# ReSharper
resharper_align_multiline_array_and_object_initializer = false
# resharper_align_multiline_array_and_object_initializer = true
resharper_align_multiline_switch_expression = false
resharper_apply_auto_detected_rules = false
resharper_arguments_style_anonymous_function_highlighting = do_not_show #check do not show
resharper_arguments_style_literal_highlighting = do_not_show
resharper_arguments_style_named_expression_highlighting = do_not_show
resharper_arguments_style_other_highlighting = do_not_show
resharper_arguments_style_string_literal_highlighting = do_not_show
resharper_arrange_attributes_highlighting = warning
resharper_arrange_static_member_qualifier_highlighting = do_not_show
resharper_arrange_this_qualifier = false
resharper_arrange_this_qualifier_highlighting = warning
resharper_arrange_trailing_comma_in_multiline_lists_highlighting = warning
resharper_arrange_trailing_comma_in_singleline_lists_highlighting = none
resharper_arrange_type_modifiers_highlighting = warning
resharper_auto_property_can_be_made_get_only_global_highlighting = do_not_show
resharper_auto_property_can_be_made_get_only_local_highlighting = do_not_show
resharper_autodetect_indent_settings = true
resharper_bad_attribute_brackets_spaces_highlighting = warning
resharper_bad_braces_spaces_highlighting = warning
resharper_bad_colon_spaces_highlighting = warning
resharper_bad_comma_spaces_highlighting = warning
resharper_bad_control_braces_line_breaks_highlighting = warning
resharper_bad_declaration_braces_line_breaks_highlighting = warning
resharper_bad_empty_braces_line_breaks_highlighting = none
